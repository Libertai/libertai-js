{
  "name": "libertai-js",
  "version": "0.0.0",
  "description": "In-browser SDK for interacting with LibertAI Decentralized AI Network",
  "keywords": [],
  "type": "module",
  "main": "lib/index.js",
  "exports": {
    ".": "./lib/index.js",
    "./lib/*": "./lib/*",
    "./*": "./lib/*",
    "./package.json": "./package.json"
  },
  "types": "lib/index.d.ts",
  "typesVersions": {
    "*": {
      "lib/index.d.ts": [
        "lib/index.d.ts"
      ],
      "lib/*": [
        "lib/*"
      ],
      "*": [
        "lib/*"
      ]
    }
  },
  "files": [
    "lib",
    "dist",
    "README.md",
    "LICENSE",
    "package.json",
    "docs"
  ],
  "author": "Alex Miller <al@krondor.org>",
  "license": "MIT",
  "engines": {
    "node": ">=18.0.0"
  },
  "scripts": {
    "lint": "yarn eslint src/**/*.ts",
    "format": "prettier --write src/**/*.ts",
    "format:check": "prettier --check src/**/*.ts",
    "prebuild": "rimraf dist",
    "build": "tsc && yarn run build:minified",
    "build:minified": "node scripts/build-minified.js",
    "start": "tsc -w",
    "prepare": "yarn build"
  },
  "devDependencies": {
    "@types/node": "^20.12.2",
    "@typescript-eslint/eslint-plugin": "^7.4.0",
    "@typescript-eslint/parser": "^7.4.0",
    "esbuild": "^0.20.2",
    "eslint": "^8.57.0",
    "prettier": "^3.0.0",
    "rimraf": "^5.0.5",
    "ts-node": "^10.2.1",
    "typescript": "^5.4.3"
  },
  "dependencies": {
    "axios": "^1.6.8",
    "langchain": "^0.1.30",
    "localforage": "^1.10.0",
    "ml-distance": "^4.0.1",
    "uuid": "^9.0.1"
  },
  "resolutions": {
    "@langchain/core": "^0.1"
  }
}
